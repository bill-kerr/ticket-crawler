{
  "name": "tickets-client",
  "version": "0.0.1",
  "scripts": {
    "build": "set NODE_ENV=production && rollup -c",
    "watch:css": "postcss public/tailwind.css -o public/styles.css -w",
    "build:css": "set NODE_ENV=production && postcss public/tailwind.css -o public/styles.css",
    "dev": "rollup -c --watch",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "autoprefixer": "^9.8.6",
    "postcss": "^8.1.6",
    "postcss-cli": "^8.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^1.9.6",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "firebase": "^8.0.1",
    "sirv-cli": "^1.0.0"
  }
}
